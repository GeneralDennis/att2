{
  "name": "frontend-boilerplate",
  "version": "0.0.0",
  "description": "frontend boilerplate",
  "scripts": {
    "start": "npm run hot",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --open --progress --hot --inline",
    "watch": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --open --progress --watch",
    "build": "npm run build:prod",
    "build:dev": "npm run development",
    "build:prod": "npm run production",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress"
  },
  "author": "",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "autoprefixer": "^9.4.10",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "extract-css-chunks-webpack-plugin": "^4.0.0",
    "extract-loader": "^2.0.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "jquery": "^3.3.1",
    "node-sass": "^4.11.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-url": "^8.0.0",
    "pug": "^2.0.3",
    "pug-html-loader": "git+https://github.com/willyelm/pug-html-loader.git",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "svgo": "^1.2.0",
    "svgo-loader": "^2.2.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "vue": "^2.6.8",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.8",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4"
  }
}
